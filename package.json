    "chalk": "^1.1.3",
    "env-test": "^1.0.0",
    "enzyme": "^2.4.1",
    "eslint": "^3.1.1",
    "eslint-import-resolver-webpack": "^0.8.1",
    "eslint-plugin-babel": "^4.1.1",
    "eslint-plugin-import": "^2.2.0",
